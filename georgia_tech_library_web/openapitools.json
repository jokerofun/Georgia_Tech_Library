{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.0.1",
    "generators": {
      "backend-api": {
        "generatorName": "typescript-fetch",
        "output": "#{cwd}/src/api",
        "glob": "swagger.json",
        "additionalProperties": {
          "supportsES6": true,
          "withInterfaces": true,
          "typescriptThreePlus": true,
          "withoutRuntimeChecks": true
        }
      }
    }
  }
}